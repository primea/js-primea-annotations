  "license": "MPL-2.0",
  "dependencies": {
    "buffer-pipe": "0.0.3",
    "leb128": "0.0.4",
    "safe-buffer": "^5.1.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "documentation": "^9.0.0",
    "minimist": "^1.2.0",
    "nyc": "^12.0.1",
    "standard": "^11.0.1",
    "tape": "^4.9.0",
    "wabt": "^1.0.0",
    "wasm-json-toolkit": "0.2.6"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:primea/primea-annotations.git"
  },
  "bugs": {
    "url": "https://github.com/primea/primea-annotations/issues"
